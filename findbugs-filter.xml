<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
    <Match>
        <Or>
            <Class name="~.*\.AllTests"/>
            <Class name="~.*TestCase"/>
        </Or>
    </Match>
    <Match>
        <Class name="deors.core.commons.DateComparator"/>
        <Field name="formatter"/>
        <Bug pattern="SE_TRANSIENT_FIELD_NOT_RESTORED"/>
    </Match>
</FindBugsFilter>
